    "iron-pony",
    "--think",
    "--wrap", "18",
    "-f", "{workspace}/testdata/ponies/simple_think.pony",
    "-b", "{workspace}/testdata/balloons/ascii.think",
    "Thinking in Rust"
  ],
  "stdin": null,
  "env": {
    "COLUMNS": "80",
    "LINES": "24",
    "PYTHONWARNINGS": "ignore"
  }
