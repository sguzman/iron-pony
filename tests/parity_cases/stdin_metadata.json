{
  "id": "stdin_metadata",
  "features": ["metadata", "mode_say", "anchor", "diff_runner"],
  "argv": ["ponysay"],
  "reference_argv": [
    "ponysay",
    "-f", "{workspace}/testdata/ponies/simple_say.pony",
    "-b", "{workspace}/testdata/balloons/ascii.say"
  ],
  "candidate_argv": [
    "iron-pony",
    "-f", "{workspace}/testdata/ponies/simple_say.pony",
    "-b", "{workspace}/testdata/balloons/ascii.say"
  ],
  "stdin": "Metadata path from stdin",
  "env": {
    "COLUMNS": "80",
    "LINES": "24",
    "PYTHONWARNINGS": "ignore"
  }
}
